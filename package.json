{
  "name": "openid_connect_server",
  "version": "1.0.0",
  "description": "runs openid connect server",
  "main": "server.mjs",
  "scripts": {
    "start": "node --experimental-modules --experimental-json-modules --no-warnings --es-module-specifier-resolution=node src/server.mjs",
    "unit-tests": "env DB_TYPE=MOCK nyc mocha -R spec test/**/*.spec.js --require esm --timeout 40000 --exit",
    "integration-tests": "mocha nyc mocha test/integration/**/*test.js --require esm --timeout 40000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tarachandverma/easy-oidc-provider"
  },
  "private": true,
  "devDependencies": {
    "express": "^4.17.1"
  },
  "dependencies": {
    "Buffer": "0.0.0",
    "aws-sdk": "^2.1046.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "esm": "^3.2.25",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lru-cache": "^5.1.1",
    "mocha": "^9.1.3",
    "moment": "^2.26.0",
    "native-dns": "^0.7.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
    "nyc": "^15.1.0",
    "querystrings": "^0.4.0",
    "rand-token": "^1.0.1",
    "request": "^2.88.2",
    "resource-router-middleware": "^0.7.0",
    "set-cookie-parser": "^2.4.8",
    "should": "^13.2.3",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.3.0",
    "uid-safe": "^2.1.5",
    "uid2": "^1.0.0",
    "uuid": "^8.1.0",
    "xml2js": "^0.4.23"
  }
}
